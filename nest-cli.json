{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "plugins": [
      {
        "name": "@nestjs/swagger/plugin",
        "options": {
          "classValidatorShim": false
        }
      }
    ],
    "assets": [
      {
        "include": "../config/*",
        "outDir": "./dist/config"
      }
    ]
  }
}
